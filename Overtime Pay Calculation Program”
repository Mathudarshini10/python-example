ovpay = 0
sum = 0

for i in range(1, 11):
    print(f"Enter Working Hours of Emp {i}:")
    h = int(input())

    if h > 40:
        extra = h - 40
        ovpay = extra * 12
        print(f"Overtime pay of Emp {i} is {ovpay}")
        sum += ovpay
    else:
        print(f"Emp {i} has no Overtime pay")

print("Total Overtime pay of all employees:", sum)

#output
Enter Working Hours of Emp 1:
5
Emp 1 has no Overtime pay
Enter Working Hours of Emp 2:
5
Emp 2 has no Overtime pay
Enter Working Hours of Emp 3:
5
Emp 3 has no Overtime pay
Enter Working Hours of Emp 4:
8
Emp 4 has no Overtime pay
Enter Working Hours of Emp 5:
9
Emp 5 has no Overtime pay
Enter Working Hours of Emp 6:
3

